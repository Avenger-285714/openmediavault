{
  "name": "openmediavault-workbench",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "build:prod": "ng build --configuration production --verbose",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint:prettier": "prettier --list-different \"src/**/*.{ts,scss}\"",
    "lint:tsc": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.spec.json --noEmit",
    "lint:eslint": "ng lint",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "run-p -cl --aggregate-output lint:*",
    "fix:prettier": "prettier --write \"{src,e2e}/**/*.{ts,scss}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:scss": "stylelint \"**/*.scss\" --fix",
    "fix": "run-s -cl fix:*",
    "e2e": "ng e2e",
    "i18n": "npm run i18n:extract",
    "i18n:extract": "ngx-translate-extract --input src --output src/assets/i18n/openmediavault-workbench.pot --clean --sort --format pot"
  },
  "private": true,
  "jest": {
    "testTimeout": 10000,
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "transform": {
      "\\.js$": "jest-esm-transformer"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!ansi-regex|strip-ansi)"
    ]
  },
  "dependencies": {
    "@angular/animations": "^12.2.11",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "^12.2.11",
    "@angular/compiler": "^12.2.11",
    "@angular/core": "^12.2.11",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^12.2.11",
    "@angular/material": "^12.2.11",
    "@angular/platform-browser": "^12.2.11",
    "@angular/platform-browser-dynamic": "^12.2.11",
    "@angular/router": "^12.2.11",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@fontsource/inter": "^4.5.0",
    "@fontsource/roboto-mono": "^4.5.0",
    "@mdi/angular-material": "^6.3.95",
    "@ngx-translate/core": "13.0.0",
    "@swimlane/ngx-datatable": "^20.0.0",
    "@types/chart.js": "^2.9.34",
    "chart.js": "^3.6.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "core-js": "3.18.3",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "ng-block-ui": "^3.0.2",
    "ngx-scrollbar": "~7.6.1",
    "ngx-toastr": "^14.1.3",
    "nunjucks": "^3.2.3",
    "rxjs": "~6.6.7",
    "strip-ansi": "7.0.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^12.1.2",
    "@angular-devkit/build-angular": "^12.2.11",
    "@angular-eslint/builder": "^12.5.0",
    "@angular-eslint/eslint-plugin": "^12.5.0",
    "@angular-eslint/eslint-plugin-template": "^12.5.0",
    "@angular-eslint/schematics": "^12.5.0",
    "@angular-eslint/template-parser": "^12.5.0",
    "@angular/cli": "^12.2.11",
    "@angular/compiler-cli": "^12.2.11",
    "@angular/language-service": "^12.2.11",
    "@biesbjerg/ngx-translate-extract": "7.0.4",
    "@types/jasmine": "^3.9.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.2",
    "@types/nunjucks": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "codelyzer": "^6.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "jasmine-core": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-esm-transformer": "^1.0.0",
    "jest-preset-angular": "^9.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "protractor": "^7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-node": "^10.3.1",
    "typescript": "~4.3.5"
  }
}
