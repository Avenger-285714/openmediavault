{
  "name": "openmediavault-workbench",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "build:prod": "ng build --prod --verbose",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint:prettier": "prettier --list-different \"src/**/*.{ts,scss}\"",
    "lint:tsc": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.spec.json --noEmit",
    "lint:eslint": "ng lint",
    "lint": "run-p -csl --aggregate-output lint:*",
    "fix:prettier": "prettier --write \"{src,e2e}/**/*.{ts,scss}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix": "run-p -csl --aggregate-output fix:*",
    "e2e": "ng e2e",
    "i18n": "npm run i18n:extract",
    "i18n:extract": "ngx-translate-extract --input src --output src/assets/i18n/openmediavault-workbench.pot --clean --sort --format pot"
  },
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "^11.2.9",
    "@angular/cdk": "^11.2.8",
    "@angular/common": "^11.2.9",
    "@angular/compiler": "^11.2.9",
    "@angular/core": "^11.2.9",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "^11.2.9",
    "@angular/material": "^11.2.8",
    "@angular/platform-browser": "^11.2.9",
    "@angular/platform-browser-dynamic": "^11.2.9",
    "@angular/router": "^11.2.9",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@mdi/angular-material": "5.9.55",
    "@ngx-translate/core": "13.0.0",
    "@swimlane/ngx-datatable": "^19.0.0",
    "@types/chart.js": "^2.9.31",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "core-js": "3.8.3",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.21",
    "ng-block-ui": "^3.0.2",
    "ngx-scrollbar": "^7.5.4",
    "ngx-toastr": "^13.2.1",
    "nunjucks": "^3.2.3",
    "rxjs": "^6.6.6",
    "strip-ansi": "^6.0.0",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.1.1",
    "@angular-devkit/build-angular": "0.1102.8",
    "@angular-eslint/builder": "^2.0.2",
    "@angular-eslint/eslint-plugin": "^2.0.2",
    "@angular-eslint/eslint-plugin-template": "^2.0.2",
    "@angular-eslint/schematics": "^2.0.2",
    "@angular-eslint/template-parser": "^2.0.2",
    "@angular/cli": "^11.2.8",
    "@angular/compiler-cli": "^11.2.9",
    "@angular/language-service": "^11.2.1",
    "@biesbjerg/ngx-translate-extract": "4.2.0",
    "@types/jasmine": "3.6.9",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "14.14.37",
    "@types/nunjucks": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "codelyzer": "6.0.1",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.3.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "jest-preset-angular": "8.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.2.1",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "typescript": "~4.1.5"
  }
}
